#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "rus");
    int a1 = 0;
    int a2 = 1;
    int n = 0;
    cout << "Введите колличество чисел фибоначи : " << endl;
    cin >> n;
    for (int i = 0; i < n; i++)
    {
        cout << a1 << endl;
        a1 = a1 + a2;
        a2 = a1 - a2;
    }
}


